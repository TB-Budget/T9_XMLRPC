inclass
  func Sort(predicate: String; const InArray:Object[]):Object[];
    var i, ii:Integer;
    --var OutArray: Object[];
    var KeyArray: Variant[2];
    var Key: Variant;
    ii = LengthOfArray(InArray);
    for i = 1 .. ii do
      Key = InArray[i].Evaluate(predicate);
      СИС2.Функции.ВставитьЗначенияВУпорядоченныйМассив(KeyArray, Key, InArray[i]);
    end;
    Result = KeyArray[СИС2.Функции.ипЗначения];
  end;

