class "Test Blog API";
import classes Client;
inclass

inobject
var RPCUrl:String;
var User:String;
var Pass:String;
var Param1:String;
var RAWREPR:String;

--------------------------------------------------------------------------------
--{ Обработчики событий бланка

  proc Button1OnClick(Sender :Button);
  var res: variant;
  var params: variant[];
    params[1] = Param1;
    params[2] = User;
    params[3] = Pass;
    res = SimpleCall(RPCUrl,'metaWeblog.getCategories',params);
    RAWREPR = Str(res);
  end;

  proc GetOnClick(Sender :Button);
  var res: variant;
  var params: variant[];
    params[1] = '';
    params[2] = User;
    params[3] = Pass;
    res = SimpleCall(RPCUrl,'blogger.getUsersBlogs',params);
    RAWREPR = Str(res);
      
  end;

  proc Button21OnClick(Sender :Button);
  var res: variant;
    res = SimpleCall(RPCUrl,'system.listMethods',[]);
RAWREPR = Str(res);
  end;

  proc signm(Sender :Button);
  var res: variant;
    res = SimpleCall(RPCUrl,'system.methodSignature',[param1]);
  RAWREPR = Str(res);
  end;

  proc Button111OnClick(Sender :Button);
  var res: variant;
  var params: variant[];
    params[1] = Param1;
    params[2] = User;
    params[3] = Pass;
    params[4] = 50;
    res = SimpleCall(RPCUrl,'metaWeblog.getRecentPosts',params);
    RAWREPR = Str(res);
  end;

--}

end